doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='author', content='Dashboard')
    meta(name='keyword', content='Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina')
    title DASHGUM - FREE Bootstrap Admin Template
    // Bootstrap core CSS
    link(href='assets/css/bootstrap.css', rel='stylesheet')
    // external css
    script(src='http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js')
    script(src='http://code.jquery.com/jquery-1.8.2.min.js')
    link(href='assets/font-awesome/css/font-awesome.css', rel='stylesheet')
    link(rel='stylesheet', type='text/css', href='assets/css/zabuto_calendar.css')
    link(rel='stylesheet', type='text/css', href='assets/js/gritter/css/jquery.gritter.css')
    link(rel='stylesheet', type='text/css', href='assets/lineicons/style.css')
    // Custom styles for this template
    link(href='assets/css/style.css', rel='stylesheet')
    link(rel='stylesheet', href='http://cdn.oesmith.co.uk/morris-0.4.3.min.css')
    link(href='assets/css/style-responsive.css', rel='stylesheet')
    script(src='assets/js/chart-master/Chart.js')
    script(src='assets/js/morris-conf.js')

    script(src='assets/js/morris-conf.js')
    script(src='javascripts/analytics.js')

    // HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body
    section#container
      //
        **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        ***********************************************************************************************************************************************************
      // header start
      header.header.black-bg
        .sidebar-toggle-box
          .fa.fa-bars.tooltips(data-placement='right', data-original-title='Toggle Navigation')
        // logo start
        a.logo(href='index.html')
          b DASHGUM FREE
        // logo end
        #top_menu.nav.notify-row
          // notification start
          ul.nav.top-menu
            // settings start
            li.dropdown
              a.dropdown-toggle(data-toggle='dropdown', href='index.html#')
                i.fa.fa-tasks
                span.badge.bg-theme 4
              ul.dropdown-menu.extended.tasks-bar
                .notify-arrow.notify-arrow-green
                li
                  p.green You have 4 pending tasks
                li
                  a(href='index.html#')
                    .task-info
                      .desc DashGum Admin Panel
                      .percent 40%
                    .progress.progress-striped
                      .progress-bar.progress-bar-success(role='progressbar', aria-valuenow='40', aria-valuemin='0', aria-valuemax='100', style='width: 40%')
                        span.sr-only 40% Complete (success)
                li
                  a(href='index.html#')
                    .task-info
                      .desc Database Update
                      .percent 60%
                    .progress.progress-striped
                      .progress-bar.progress-bar-warning(role='progressbar', aria-valuenow='60', aria-valuemin='0', aria-valuemax='100', style='width: 60%')
                        span.sr-only 60% Complete (warning)
                li
                  a(href='index.html#')
                    .task-info
                      .desc Product Development
                      .percent 80%
                    .progress.progress-striped
                      .progress-bar.progress-bar-info(role='progressbar', aria-valuenow='80', aria-valuemin='0', aria-valuemax='100', style='width: 80%')
                        span.sr-only 80% Complete
                li
                  a(href='index.html#')
                    .task-info
                      .desc Payments Sent
                      .percent 70%
                    .progress.progress-striped
                      .progress-bar.progress-bar-danger(role='progressbar', aria-valuenow='70', aria-valuemin='0', aria-valuemax='100', style='width: 70%')
                        span.sr-only 70% Complete (Important)
                li.external
                  a(href='#') See All Tasks
            // settings end

          // notification end
        .top-menu
          ul.nav.pull-right.top-menu
            li
              a.logout(href='login.html') Logout
      // header end
      //
        **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        ***********************************************************************************************************************************************************
      // sidebar start
      aside
        #sidebar.nav-collapse
          // sidebar menu start
          ul#nav-accordion.sidebar-menu
            p.centered
              a(href='profile.html')
                img.img-circle(src='assets/img/ui-sam.jpg', width='60')
            h5.centered Marcel Newman
            li.mt
              a.active(href='index.html')
                i.fa.fa-dashboard
                span Dashboard

          // sidebar menu end
      // sidebar end
      //
        **********************************************************************************************************************************************************
        MAIN CONTENT
        ***********************************************************************************************************************************************************
      // main content start
      section#main-content
        section.wrapper
          .row
            .col-lg-9.main-chart
              .row.mtbox
                .col-md-2.col-sm-2.col-md-offset-1.box0
                  .box1
                    span.li_heart
                    h3 933
                  p 933 People liked your page the last 24hs. Whoohoo!
                .col-md-2.col-sm-2.box0
                  .box1
                    span.li_cloud
                    h3 +48
                  p 48 New files were added in your cloud storage.
                .col-md-2.col-sm-2.box0
                  .box1
                    span.li_stack
                    h3 23
                  p You have 23 unread messages in your inbox.
                .col-md-2.col-sm-2.box0
                  .box1
                    span.li_news
                    h3 +10
                  p More than 10 news were added in your reader.
                .col-md-2.col-sm-2.box0
                  .box1
                    span.li_data
                    h3 OK!
                  p Your server is working perfectly. Relax & enjoy.
              .row.mt
                .col-lg-12
                  .content-panel
                    table.table.table-striped.table-advance.table-hover
                      h4
                        i.fa.fa-angle-right
                        |  Bills
                      hr
                      thead
                        tr
                          th
                            i.fa.fa-bullhorn
                            |  Company
                          th.hidden-phone
                            i.fa.fa-question-circle
                            |  Descrition
                          th
                            i.fa.fa-bookmark
                            |  Profit
                          th
                            i.fa.fa-edit
                            |  Status
                          th
                      tbody
                        each transaction, index in transactions
                          tr
                            td
                              a(href='basic_table.html#')=(transaction.to)
                            td=('$' + transaction.value)
                            td
                              span.label.label-info.label-mini Due
                            td
                              button.btn.btn-success.btn-xs
                                i.fa.fa-check
                              button.btn.btn-primary.btn-xs
                                i.fa.fa-pencil
                              button.btn.btn-danger.btn-xs
                                i.fa.fa-trash-o
                  .row.mt
                    .col-lg-12
                      .content-panel
                          table.table.table-striped.table-advance.table-hover
                            h4
                              i.fa.fa-angle-right
                              |  Recent Transactions
                            hr
                            thead
                              tr
                                th
                                  i.fa.fa-bullhorn
                                  |  Company
                                th.hidden-phone
                                  i.fa.fa-question-circle
                                  |  Descrition
                                th
                                  i.fa.fa-bookmark
                                  |  Profit
                                th
                                  i.fa.fa-edit
                                  |  Status
                                th
                            tbody
                              each transaction, index in transactions
                                tr
                                  td
                                    a(href='basic_table.html#')=(transaction.to)
                                  td=('$' + transaction.value)
                                  td
                                    span.label.label-info.label-mini Due
                                  td
                                    button.btn.btn-success.btn-xs
                                      i.fa.fa-check
                                    button.btn.btn-primary.btn-xs
                                      i.fa.fa-pencil
                                    button.btn.btn-danger.btn-xs
                                      i.fa.fa-trash-o


      // main content end
      // footer start
      footer.site-footer
        .text-center
          | 2014 - Alvarez.is
          a.go-top(href='index.html#')
            i.fa.fa-angle-up
      // footer end
    // js placed at the end of the document so the pages load faster
    script(src='assets/js/jquery.js')
    script(src='assets/js/jquery-1.8.3.min.js')
    script(src='assets/js/bootstrap.min.js')
    script.include(type='text/javascript', src='assets/js/jquery.dcjqaccordion.2.7.js')
    script(src='assets/js/jquery.scrollTo.min.js')
    script(src='assets/js/jquery.nicescroll.js', type='text/javascript')
    script(src='assets/js/jquery.sparkline.js')
    // common script for all pages
    script(src='assets/js/common-scripts.js')
    script(type='text/javascript', src='assets/js/gritter/js/jquery.gritter.js')
    script(type='text/javascript', src='assets/js/gritter-conf.js')
    // script for this page
    script(src='assets/js/sparkline-chart.js')
    script(src='assets/js/zabuto_calendar.js')

