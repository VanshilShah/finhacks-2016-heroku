                            button.btn.btn-danger.btn-xs
                              i.fa.fa-trash-o
                        tr
                          td
                            a(href='basic_table.html#')
                              | Dashgum co
                          td.hidden-phone Lorem Ipsum dolor
                          td 17900.00$
                          td
                            span.label.label-warning.label-mini Due
                          td
                            button.btn.btn-success.btn-xs
                              i.fa.fa-check
                            button.btn.btn-primary.btn-xs
                              i.fa.fa-pencil
                            button.btn.btn-danger.btn-xs
                              i.fa.fa-trash-o
                        tr
                          td
                            a(href='basic_table.html#')
                              | Another Co
                          td.hidden-phone Lorem Ipsum dolor
                          td 14400.00$
                          td
                            span.label.label-success.label-mini Paid
                          td
                            button.btn.btn-success.btn-xs
                              i.fa.fa-check
                            button.btn.btn-primary.btn-xs
                              i.fa.fa-pencil
                            button.btn.btn-danger.btn-xs
                              i.fa.fa-trash-o
                        tr
                          td
                            a(href='basic_table.html#')
                              | Dashgum ext
                          td.hidden-phone Lorem Ipsum dolor
                          td 22000.50$
                          td
                            span.label.label-success.label-mini Paid
                          td
                            button.btn.btn-success.btn-xs
                              i.fa.fa-check
                            button.btn.btn-primary.btn-xs
                              i.fa.fa-pencil
                            button.btn.btn-danger.btn-xs
                              i.fa.fa-trash-o
                        tr
                          td
                            a(href='basic_table.html#') Total Ltd
                          td.hidden-phone Lorem Ipsum dolor
                          td 12120.00$
                          td
                            span.label.label-warning.label-mini Due
                          td
                            button.btn.btn-success.btn-xs
                              i.fa.fa-check
                            button.btn.btn-primary.btn-xs
                              i.fa.fa-pencil
                            button.btn.btn-danger.btn-xs
                              i.fa.fa-trash-o
                  // /content-panel
                // /col-md-12
              // /row

              .row.mt
                // custom chart end
              // /row
            // /col-lg-9 END SECTION MIDDLE
            //
              **********************************************************************************************************************************************************
              RIGHT SIDEBAR CONTENT
              ***********************************************************************************************************************************************************
            .col-lg-3.ds
              // COMPLETED ACTIONS DONUTS CHART
              h3 TIMELINE
              // /col-lg-3
      // main content end
      // footer start
      footer.site-footer
        .text-center
          | 2014 - Alvarez.is
          a.go-top(href='index.html#')
            i.fa.fa-angle-up
      // footer end
